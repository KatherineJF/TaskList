
<h1> All Tasks</h1>
<%= link_to "Add Task", new_task_path %>
<%= link_to "Completed Tasks", completed_tasks_path %>
<%= link_to "Mark as Complete", complete_task_path(@task.id), method: :put %>
<ul>
  <% @tasks.each do |task|  %>
  <li>
      <%= link_to task.name, task_path(task.id) %>
    </li>
  <% end %>
</ul>
